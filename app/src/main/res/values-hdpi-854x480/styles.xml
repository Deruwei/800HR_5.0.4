<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>
    <!--主页tab标签 linearlayout-->
    <style name="linear_main_tab">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>
    <!-- 简历头像-->
    <style name="AlertDialogCustom" parent="@android:style/Theme.Dialog">
        <item name="android:textSize">20sp</item>
        <item name="android:paddingBottom">0dip</item>
        <item name="android:paddingTop">0dip</item>
        <item name="android:topOffset">0dip</item>
        <item name="android:clipToPadding">true</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="linear_main_tab_tv">
        <item name="android:textColor">#333333</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text_shadow">
        <item name="android:shadowColor">#949390</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.5</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="linear_main_tab_iv">
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <style name="check_huadong_lvhui2" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/check_huadong_lvhui</item>
    </style>
    <!--我 页面linearLayout-->
    <style name="linear_me_width">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="linear_me_height">
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!--发现页面-->
    <style name="text_find_tab">
        <item name="android:gravity">center</item>
        <item name="android:layout_height">43dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="checkbox_territory_style">
        <item name="android:button">@drawable/checkbox_bg</item>
    </style>

    <style name="linear_find_width">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="linear_find_view">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">90dp</item>
    </style>

    <style name="TitleBar">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/dialog_button_text_color</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">@dimen/base_title_bar_height</item>
    </style>
    <!-- 这里设立windowBackground的背景后就不用在xml里再设置了 -->
    <style name="MyDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@drawable/common_alert_bg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="colour_style2">
        <item name="android:textColor">#949390</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="ui_parent_Wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="cityLocatingProgressStyle">
        <item name="android:maxWidth">16.0dip</item>
        <item name="android:maxHeight">24.0dip</item>
        <item name="android:indeterminateDrawable">@anim/city_locating_anim</item>
        <item name="android:minWidth">16.0dip</item>
        <item name="android:minHeight">24.0dip</item>
    </style>

    <style name="city_hot_header" parent="@style/ui_parent_Wrap">
        <item name="android:textSize">@dimen/city_list_text_size</item>
        <item name="android:textColor">@color/city_item_letter_color</item>
        <item name="android:duplicateParentState">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="checkbox_searchjoblv">
        <item name="android:button">@drawable/checkbox_searchrjoblv_style</item>
    </style>

    <style name="checkbox_popupfiltre">
        <item name="android:button">@drawable/checkbox_popupfiltrate_style</item>
    </style>
    <!--自定义dialog样式-->
    <style name="dialog_poster" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@mipmap/bg_black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimAmount">0.3</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <!--自定义简历-->
    <style name="resume_myresume_relative1">
        <item name="android:background">#fff</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="resume_myresume_relative2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="resume_myresume_textviewresume">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="resume_myresume_textviewresume2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#666666</item>
    </style>

    <style name="resume_myresume_textviewresumedian">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#F33900</item>
        <item name="android:text">&#160;*&#160;</item>
        <item name="android:gravity">center</item>
    </style>
    <!--预览简历页面-->
    <style name="resume_previewresume_title">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="resume_previewresume_infoname">
        <item name="android:textColor">#999999</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="resume_previewresume_info">
        <item name="android:paddingRight">10dp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="reusme_myresume_tvhead">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginLeft">12dp</item>
    </style>

    <!-- 我的页面 图标-->
    <style name="me_fragment_iv_icon">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginTop">11dp</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="me_fragment_tv_icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">7dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textSize">12sp</item>
    </style>
    <!--找工作页面历史记录框-->
    <style name="pager_keyword_search_history">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <!--橙条按钮-->
    <style name="bt_orange">
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_height">40dp</item>
    </style>
    <style name="bt_orange_two">
        <item name="android:layout_width">140dp</item>
        <item name="android:layout_height">40dp</item>
    </style>
    <!--关键词搜索页面搜索框-->
    <style name="pager_keyword_search_search">
        <item name="android:layout_width">260dp</item>
        <item name="android:layout_height">30dp</item>
    </style>
    <!--关键词搜索页面搜索框-->
    <style name="pager_keyword_search_searchscale">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#666666</item>
    </style>
    <!-- 标题栏-->
    <style name="title_tv_textsize">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="title_rl_scale">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@color/new_main</item>
    </style>

    <style name="title_iv_back">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@mipmap/nav_fanhui</item>
    </style>
    <style name="change_width">
        <item name="android:layout_width">300dp</item>
    </style>
    <!-- 搜索框距离-->
    <style name="search_icon_distance">
        <item name="android:layout_marginLeft">10dp</item>
    </style>
</resources>
