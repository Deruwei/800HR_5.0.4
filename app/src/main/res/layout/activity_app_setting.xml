<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6F6"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context="com.hr.ui.activity.AppSettingActivity">

    <RelativeLayout
        android:id="@+id/rl_settingapp_top"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@color/new_main">
        <ImageView
            android:id="@+id/iv_settingapp_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/nav_fanhui" />
        <TextView
            android:id="@+id/tv_settingapp_jobnum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="设置"
            android:textColor="#fff"
            android:textSize="22sp" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#d3d3d3" />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <RelativeLayout
        android:id="@+id/rl_settingapp_autologin1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginRight="@dimen/dimen_5px"
        android:layout_marginLeft="@dimen/dimen_5px"
        android:background="@drawable/bg_white_circle"
        android:visibility="visible">

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="自动登录"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/iv_settingapp_autologin1"
            android:layout_width="40dp"
            android:layout_height="24dp"
            android:layout_alignBottom="@+id/textView37"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="12dp"
            android:layout_marginRight="12dp"
            android:src="@mipmap/kaiguan_kai" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_5px" />

    <RelativeLayout
        android:id="@+id/rl_settingapp_note"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#fff"
        android:visibility="gone">

        <TextView
            android:id="@+id/textView371"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="关注我们"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/iv_settingapp_autologin"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_marginRight="@dimen/dimen_5px"
        android:layout_marginLeft="@dimen/dimen_5px"
        android:background="@drawable/bg_white_circle"
        android:id="@+id/rl_settingapp_attention"
        android:layout_width="match_parent"
        android:layout_height="50dp">

        <TextView
            android:id="@+id/textView372333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="关注我们"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/iv_settingapp_attention"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />

        <View
            android:layout_width="match_parent"
            android:layout_marginLeft="@dimen/dimen_10px"
            android:layout_marginRight="@dimen/dimen_10px"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="#EBEBEB" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="@dimen/dimen_5px"
        android:id="@+id/rl_settingapp_feedback"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginRight="@dimen/dimen_5px"
        android:layout_marginLeft="@dimen/dimen_5px"
        android:background="@drawable/bg_white_circle">

        <TextView
            android:id="@+id/textView372"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="用户反馈"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/iv_settingapp_autologin111"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_settingapp_recommend"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#fff"
        android:visibility="gone">

        <TextView
            android:id="@+id/textView3721"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="推荐应用"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/iv_settingapp_autologin2"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="@dimen/dimen_5px"
        android:id="@+id/rl_settingapp_regard"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginRight="@dimen/dimen_5px"
        android:layout_marginLeft="@dimen/dimen_5px"
        android:background="@drawable/bg_white_circle">

        <TextView
            android:id="@+id/textView373"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="关于800hr"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/iv_settingapp_autologin3"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="@dimen/dimen_5px"
        android:id="@+id/rl_settingapp_comregister"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginRight="@dimen/dimen_5px"
        android:layout_marginLeft="@dimen/dimen_5px"
        android:background="@drawable/bg_white_circle">

        <TextView
            android:id="@+id/textView3736"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="企业招聘注册"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/iv_settingapp_comregister"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/dimen_5px"
        android:id="@+id/rl_settingapp_mark"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginRight="@dimen/dimen_5px"
        android:layout_marginLeft="@dimen/dimen_5px"
        android:background="@drawable/bg_white_circle">

        <TextView
            android:id="@+id/textView3737"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="给我们打分"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/iv_settingapp_mark"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_5px" />

    <RelativeLayout
        android:id="@+id/rl_settingapp_clear"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginRight="@dimen/dimen_5px"
        android:layout_marginLeft="@dimen/dimen_5px"
        android:background="@drawable/bg_white_circle">

        <TextView
            android:id="@+id/textView374"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="清理缓存"
            android:textColor="#666666"
            android:textSize="18sp" />

        <ImageView
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_20px" />

    <RelativeLayout
        android:id="@+id/rl_settingapp_out"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginRight="@dimen/dimen_5px"
        android:layout_marginLeft="@dimen/dimen_5px"
        android:background="@drawable/bg_white_circle"
        android:visibility="gone">

        <TextView
            android:id="@+id/tv_settingapp_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="退出登录"
            android:textColor="#666666"
            android:textSize="18sp" />
    </RelativeLayout>
</LinearLayout>
