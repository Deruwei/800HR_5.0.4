                        android:layout_toRightOf="@+id/tv_resumePersonInfo_CMName">

                        <TextView
                            android:id="@+id/tv_resume_personinfo_certificateNumberType"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:text="请选择"
                            android:textColor="@color/text_color_999"
                            android:textSize="14sp" />

                        <ImageView
                            android:id="@+id/imageViewcertificateNumberType"
                            android:layout_width="6dp"
                            android:layout_height="10dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dimen_5px"
                            android:layout_toRightOf="@+id/tv_resume_personinfo_certificateNumberType"
                            android:src="@mipmap/jiantou_right" />
                    </RelativeLayout>

                    <EditText
                        android:id="@+id/et_resume_personinfo_certificateNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="请填写号码"
                        android:textColorHint="@color/text_color_999"
                        android:textSize="14sp" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="@dimen/dimen_10px"
                    android:layout_marginRight="@dimen/dimen_10px"
                    android:background="#EBEBEB" />
                <RelativeLayout
                    android:id="@+id/rl_resumePersonInfo_address"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_50px">
                    <TextView
                        style="@style/reusme_myresume_tvhead2"
                        android:text="@string/address"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <EditText
                        android:id="@+id/et_resume_personinfo_address"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="请填写"
                        android:textColorHint="@color/text_color_999"
                        android:textSize="14sp" />
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="@dimen/dimen_10px"
                    android:layout_marginRight="@dimen/dimen_10px"
                    android:background="#EBEBEB" />
                <RelativeLayout
                    android:id="@+id/rl_resumePersonInfo_myHomePage"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_50px">
                    <TextView
                        style="@style/reusme_myresume_tvhead2"
                        android:text="@string/message_homepage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <EditText
                        android:id="@+id/et_resume_personinfo_myHomePage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="请填写"
                        android:textColorHint="@color/text_color_999"
                        android:textSize="14sp" />
                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

    <RelativeLayout
        android:paddingBottom="@dimen/dimen_5px"
        android:paddingTop="@dimen/dimen_5px"
        android:layout_alignParentBottom="true"
        android:id="@+id/rl_myresume_savenext1"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="#EBEBEB">

        <RelativeLayout
            android:layout_centerHorizontal="true"
            android:id="@+id/rl_resume_personinfo_save"
            style="@style/bt_orange"
            android:background="@drawable/btn_1chang"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="保存修改"
                android:textColor="#fff"
                android:textSize="18sp" />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
