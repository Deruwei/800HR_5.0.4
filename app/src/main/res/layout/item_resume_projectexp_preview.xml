<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:paddingRight="@dimen/dimen_14px"
    android:layout_height="match_parent"
    android:orientation="horizontal">
    <View
        android:layout_width="@dimen/dimen_14px"
        android:layout_height="match_parent" />

    <LinearLayout
        android:gravity="center_horizontal"
        android:layout_width="10dp"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <View
            android:layout_width="1dp"
            android:layout_height="14dp"
            />
        <ImageView
            android:layout_width="9dp"
            android:layout_height="9dp"
            android:layout_alignBottom="@+id/tv_previewresume_traintime"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:src="@mipmap/jianli_yulan_xiaoquan" />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#EBEBEB" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tv_myresume_project_modify"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@mipmap/xiugai1"
                android:drawablePadding="5dp"
                android:text="编辑"
                android:textColor="#F39700"
                android:layout_alignTop="@+id/tv_previewresume_traintime"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <TextView
                android:id="@+id/tv_previewresume_traintime"
                style="@style/resume_previewresume_infoname"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="7dp"
                android:layout_marginStart="7dp"
                android:layout_marginTop="11dp"
                android:text="2006年12月-2012年12月" />


            <TextView
                android:id="@+id/tv_previewresume_trainname"
                style="@style/resume_previewresume_infoname"
                android:layout_alignBaseline="@+id/tv_previewresume_jobexppost"
                android:layout_alignBottom="@+id/tv_previewresume_jobexppost"
                android:layout_toEndOf="@+id/tv_previewresume_jobexppost"
                android:layout_toRightOf="@+id/tv_previewresume_jobexppost" />

            <TextView
                android:id="@+id/textView10"
                style="@style/resume_previewresume_infoname"
                android:layout_alignLeft="@+id/tv_previewresume_traintime"
                android:layout_alignStart="@+id/tv_previewresume_traintime"
                android:layout_below="@+id/tv_previewresume_traintime"
                android:layout_marginTop="12dp"
                android:text="项目名称：" />

            <TextView
                android:id="@+id/tv_previewresume_projectname"
                style="@style/resume_previewresume_infoname"
                android:layout_alignTop="@+id/textView10"
                android:layout_toEndOf="@+id/textView10"
                android:layout_toRightOf="@+id/textView10" />

            <TextView
                android:id="@+id/tv_previewresume_jobexppost"
                style="@style/resume_previewresume_infoname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView10"
                android:layout_alignStart="@+id/textView10"
                android:layout_below="@+id/tv_previewresume_projectname"
                android:layout_marginTop="12dp"
                android:text="项目职务：" />

            <TextView
                android:id="@+id/textView13"
                style="@style/resume_previewresume_infoname"
                android:layout_alignLeft="@+id/tv_previewresume_jobexppost"
                android:layout_alignStart="@+id/tv_previewresume_jobexppost"
                android:layout_below="@+id/tv_previewresume_jobexppost"
                android:layout_marginTop="12dp"
                android:text="项目描述：" />


            <TextView
                android:id="@+id/tv_previewresume_projectdesp"
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView13"
                android:layout_alignStart="@+id/textView13"
                android:layout_below="@+id/textView13"
                android:layout_marginTop="12dp"
                android:gravity="left"
                android:text="项目描述"
                android:textColor="#999999"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textView12"
                style="@style/resume_previewresume_infoname"
                android:layout_alignLeft="@+id/tv_previewresume_projectdesp"
                android:layout_alignStart="@+id/tv_previewresume_projectdesp"
                android:layout_below="@+id/tv_previewresume_projectdesp"
                android:layout_marginTop="12dp"
                android:text="项目职责：" />


            <TextView
                android:id="@+id/tv_previewresume_projectduty"
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView12"
                android:layout_alignStart="@+id/textView12"
                android:layout_below="@+id/textView12"
                android:layout_marginTop="12dp"
                android:gravity="left"
                android:text="项目职责"
                android:textColor="#999999"
                android:textSize="15sp" />

        </RelativeLayout>

        <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#fff" />
        </LinearLayout>
</LinearLayout>