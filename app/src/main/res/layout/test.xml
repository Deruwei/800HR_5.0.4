<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#f6f6f6" />

    <RelativeLayout
        android:id="@+id/rl_preview_resume_train"
        android:layout_width="360dp"
        android:layout_height="match_parent"
        android:background="#fff">

        <RelativeLayout
            android:layout_width="360dp"
            android:layout_height="match_parent"
            android:background="#fff">

            <RelativeLayout
                android:id="@+id/rl_myresume_113"
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:background="#EEEEEE"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/imageView50rl_myresume_113"
                    android:layout_width="7dp"
                    android:layout_height="16dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="14dp"
                    android:layout_marginStart="14dp"
                    android:src="@mipmap/jianli_yulan_sutiao" />
                <TextView
                    style="@style/resume_previewresume_title"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_marginStart="6dp"
                    android:layout_toEndOf="@+id/imageView50rl_myresume_113"
                    android:layout_toRightOf="@+id/imageView50rl_myresume_113"
                    android:text="@string/myresume_privewresume_infoname8" />
            </RelativeLayout>

            <View
                android:id="@+id/rl_myresume_213"
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:layout_below="@+id/rl_myresume_113"
                android:background="#fff" />

            <LinearLayout
                android:id="@+id/activity_resume_scan_train_linnearlayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rl_myresume_213"
                android:focusableInTouchMode="false"
                android:orientation="vertical"></LinearLayout>
        </RelativeLayout>
    </RelativeLayout>

</LinearLayout>