<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">
    <View
        android:layout_width="24dp"
        android:layout_height="match_parent" />

    <LinearLayout
        android:gravity="center_horizontal"
        android:layout_width="10dp"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <View
            android:layout_width="1dp"
            android:layout_height="14dp"
            />
        <ImageView
            android:layout_width="9dp"
            android:layout_height="9dp"
            android:layout_alignBottom="@+id/tv_previewresume_traintime"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:src="@mipmap/jianli_yulan_xiaoquan" />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#EBEBEB" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/tv_myresume_langue_modify"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="23dp"
            android:drawableLeft="@mipmap/xiugai1"
            android:drawablePadding="5dp"
            android:text="编辑"
            android:textColor="#F39700"
            android:layout_marginEnd="10dp"
            android:layout_alignTop="@+id/tv_previewresume_languagename"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <TextView
            android:id="@+id/tv_previewresume_languagespeaklevel"
            style="@style/resume_previewresume_infoname"
            android:layout_alignTop="@+id/tv_previewresume_jobexppost"
            android:layout_toRightOf="@+id/tv_previewresume_jobexppost"
            android:layout_toEndOf="@+id/tv_previewresume_jobexppost" />

        <TextView
            android:id="@+id/textView10"
            style="@style/resume_previewresume_infoname"
            android:text="语言类别："
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="8dp" />

        <TextView
            android:id="@+id/tv_previewresume_languagename"
            style="@style/resume_previewresume_infoname"
            android:layout_alignTop="@+id/textView10"
            android:layout_toEndOf="@+id/textView10"
            android:layout_toRightOf="@+id/textView10" />

        <TextView
            android:id="@+id/tv_previewresume_jobexppost"
            style="@style/resume_previewresume_infoname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView10"
            android:layout_alignStart="@+id/textView10"
            android:layout_below="@+id/textView10"
            android:layout_marginTop="9dp"
            android:text="听说能力：" />

        <TextView
            android:id="@+id/tv_previewresume_trainname"
            style="@style/resume_previewresume_infoname"
            android:layout_marginTop="12dp"
            android:text="读写能力："
            android:layout_below="@+id/tv_previewresume_languagespeaklevel"
            android:layout_alignLeft="@+id/tv_previewresume_jobexppost"
            android:layout_alignStart="@+id/tv_previewresume_jobexppost" />

        <TextView
            android:id="@+id/tv_previewresume_languagereadlevel"
            style="@style/resume_previewresume_infoname"
            android:layout_alignTop="@+id/tv_previewresume_trainname"
            android:layout_toRightOf="@+id/tv_previewresume_trainname"
            android:layout_toEndOf="@+id/tv_previewresume_trainname" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#fff" />
        </LinearLayout>
</LinearLayout>