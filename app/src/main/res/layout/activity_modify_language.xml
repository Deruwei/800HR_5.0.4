<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context="com.hr.ui.activity.ModifyLanguageActivity">

    <RelativeLayout
        android:id="@+id/rl_resume_modifynewlangue_top"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:background="@color/new_main"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="0dp">

        <ImageView
            android:id="@+id/iv_resume_modifynewlangue_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/nav_fanhui" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="语言能力"
            android:textColor="#fff"
            android:textSize="22sp" />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rl_resume_modifynewlangue_top"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical">
            <TextView
                android:id="@+id/textView42"
                style="@style/reusme_myresume_tvhead"
                android:text="语言类别" />
            <TextView
                android:id="@+id/sp_resume_modifylanguage_langname"
                android:layout_width="@dimen/dimen_100px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical|right"
                android:layout_marginEnd="@dimen/dimen_13px"
                android:layout_marginRight="@dimen/dimen_13px"
                android:layout_toLeftOf="@+id/imageView45"
                android:layout_toStartOf="@+id/imageView45"
                android:background="@null"
                android:gravity="center_vertical|right"
                android:singleLine="true"
                android:text="请选择语言类别"
                android:textColor="#F7931E"/>
            <ImageView
                android:id="@+id/imageView45"
                android:layout_width="6dp"
                android:layout_height="10dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="12dp"
                android:layout_marginRight="12dp"
                android:src="@mipmap/jiantou_right" />
        </RelativeLayout>
        <View
            android:layout_marginLeft="13dp"
            android:layout_marginStart="13dp"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:background="#efefef"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1px"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical">
            <TextView
                android:id="@+id/textView42444"
                style="@style/reusme_myresume_tvhead"
                android:text="听说能力" />
            <TextView
                android:id="@+id/sp_resume_modifylanguage_speak_level"
                android:layout_width="@dimen/dimen_100px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical|right"
                android:layout_marginEnd="@dimen/dimen_13px"
                android:layout_marginRight="@dimen/dimen_13px"
                android:layout_toLeftOf="@+id/imageView452222"
                android:layout_toStartOf="@+id/imageView452222"
                android:background="@null"
                android:gravity="center_vertical|right"
                android:singleLine="true"
                android:text="请选择听说能力"
                android:textColor="#F7931E"/>

            <ImageView
                android:id="@+id/imageView452222"
                android:layout_width="6dp"
                android:layout_height="10dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="12dp"
                android:layout_marginRight="12dp"
                android:src="@mipmap/jiantou_right" />
        </RelativeLayout>
        <View
            android:layout_marginLeft="13dp"
            android:layout_marginStart="13dp"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:background="#efefef"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1px" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/textView42334"
                style="@style/reusme_myresume_tvhead"
                android:text="读写能力" />

            <TextView
                android:id="@+id/sp_resume_modifylanguage_read_level"
                android:layout_width="@dimen/dimen_100px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical|right"
                android:layout_marginEnd="@dimen/dimen_13px"
                android:layout_marginRight="@dimen/dimen_13px"
                android:layout_toLeftOf="@+id/imageView10045"
                android:layout_toStartOf="@+id/imageView10045"
                android:background="@null"
                android:gravity="center_vertical|right"
                android:singleLine="true"
                android:text="请选择读写能力"
                android:textColor="#F7931E" />

            <ImageView
                android:id="@+id/imageView10045"
                android:layout_width="6dp"
                android:layout_height="10dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="12dp"
                android:layout_marginRight="12dp"
                android:src="@mipmap/jiantou_right" />

        </RelativeLayout>
        <View
            android:layout_marginLeft="13dp"
            android:layout_marginStart="13dp"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:background="#efefef"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1px"  />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">
          <RelativeLayout
              android:layout_weight="1"
              android:layout_width="0dp"
              android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_resume_item_languagemodify_delete"
                android:layout_marginLeft="@dimen/dimen_20px"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_40px"
                android:paddingLeft="@dimen/dimen_50px"
                android:paddingRight="@dimen/dimen_50px"
                android:background="@drawable/btn_1chang"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/delete"
                android:textColor="#fff"
                android:textSize="14sp"/>
          </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_resume_item_llanguagemodify_save"
                android:layout_marginLeft="@dimen/dimen_20px"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_40px"
                android:paddingLeft="@dimen/dimen_50px"
                android:paddingRight="@dimen/dimen_50px"
                android:background="@drawable/btn_1chang"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="保存"
                android:textColor="#fff"
                android:textSize="14sp" />
        </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
